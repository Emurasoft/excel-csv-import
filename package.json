{
  "name": "excel-csv-import",
  "version": "0.0.0",
  "repository": "https://github.com/Emurasoft/excel-csv-import.git",
  "author": "Makoto",
  "license": "MIT",
  "scripts": {
    "test": "TS_NODE_FILES=true TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' mocha $NODE_DEBUG_OPTION -r ts-node/register,test/setup.ts **/*.test.ts **/*.test.tsx",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "devServer": "webpack-dev-server --config webpack.dev.js",
    "validateManifest": "validate-office-addin manifests/dev.manifest.xml && validate-office-addin manifests/prod.manifest.xml",
    "build": "rm -rf build && webpack --config webpack.prod.js",
    "generateLicensesText": "echo 'export default `'\"$(yarn licenses generate-disclaimer | tail -n +7 | sed -e 's/`/\\\\`/g')\"'`;' > src/licenses/thirdParty.ts"
  },
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@microsoft/office-js": "^1.1.10",
    "@types/file-saver": "^2.0.0",
    "@types/office-js": "0.0.166",
    "@types/papaparse": "^4.5.9",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.1",
    "core-js": "^3.0.0",
    "css-loader": "^2.1.1",
    "file-saver": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "office-ui-fabric-react": "^6.152.1",
    "papaparse": "^4.6.3",
    "query-string": "^5.1.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.9.0",
    "@types/mocha": "^5.2.6",
    "@types/sinon": "^7.0.10",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "@typescript-eslint/typescript-estree": "^1.4.2",
    "assert": "^1.4.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.1",
    "eslint-plugin-react": "^7.12.4",
    "ignore-styles": "^5.0.1",
    "jsdom": "^14.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "office-addin-validator": "^1.0.5",
    "retrace": "^1.0.0",
    "sinon": "^7.2.7",
    "ts-node": "^8.0.3",
    "webpack-dev-server": "^3.2.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
